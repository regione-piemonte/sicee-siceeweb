																				</xs:annotation>
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="confineEsterno">
																							<xs:complexType>
																								<xs:sequence>
																									<xs:element name="superficieOpaca" minOccurs="0" maxOccurs="unbounded">
																										<xs:complexType>
																											<xs:sequence>
																												<xs:element name="descrizione" type="xs:string" minOccurs="0"/>
																												<xs:element name="tipo" default="0">
																													<xs:annotation>
																														<xs:documentation>0=Parete
1=Pavimento
2=Soffitto
3=Porta
4=Parete Speciale (isolamento trasparente)
5=Parete Speciale (muro di trombe)
6=Parete Speciale (facciata ventilata)</xs:documentation>
																													</xs:annotation>
																													<xs:simpleType>
																														<xs:restriction base="xs:unsignedByte">
																															<xs:minInclusive value="0"/>
																															<xs:maxInclusive value="6"/>
																														</xs:restriction>
																													</xs:simpleType>
																												</xs:element>
																												<xs:element name="superficieDisperdente">
																													<xs:annotation>
																														<xs:documentation>Superficie netta disperdente
[m²]</xs:documentation>
																													</xs:annotation>
																													<xs:simpleType>
																														<xs:restriction base="xs:decimal">
																															<xs:minInclusive value="0"/>
																														</xs:restriction>
																													</xs:simpleType>
																												</xs:element>
																												<xs:element name="trasmittanza" type="typeTrasmittanza">
																													<xs:annotation>
																														<xs:documentation>Trasmittanza della superficie
[W/m²K]</xs:documentation>
																													</xs:annotation>
																												</xs:element>
																												<xs:element name="fonteTrasmittanza">
																													<xs:annotation>
																														<xs:documentation>0=Prospetto
1=Progetto
2=Da produttore</xs:documentation>
																													</xs:annotation>
																													<xs:simpleType>
																														<xs:restriction base="xs:unsignedByte">
																															<xs:minInclusive value="0"/>
